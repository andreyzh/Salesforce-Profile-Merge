<Window x:Class="Wyndnet.SFDC.ProfileMerge.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wyndnet.SFDC.ProfileMerge"
        mc:Ignorable="d"
        Title="StartWindow" Height="300" Width="300">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,5,0,0">
            <Button x:Name="ButtonProfileMergeLaunch" Content="Profile Merge" Width="120" Click="ButtonProfileMergeLaunch_Click"/>
            <Button Content="Reference Removal" Width="120" Margin="0,5,0,0"/>
        </StackPanel>
    </Grid>
</Window>
